
public class ReverseString {
	
  public String reversestring(String str){
		char[] array = str.toCharArray();
		int i = 0;
		int j = str.length() - 1;
		char temp;
		
		while (i < j){
			temp = array[i];
			array[i] = array[j];
			array[j] = temp;
			i++;
			j--;
		
    }
	}
	return new String(array);
}
